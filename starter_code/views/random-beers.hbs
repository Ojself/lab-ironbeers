<div class="randomCont">
        <div class="col-left">
      <img class="randomImage"src="{{image_url}}">
        </div>
    <div class="col-right">
        <h3>{{name}}</h3>
        <h4>{{tagline}}</h4>
          <p>{{description}}</p>
        <h3><b>Suggested food:</b></h3>
        <ul>
          {{#each food_pairing}}
              <li>{{this}}</li>
          {{/each}}
        </ul>
        <h3><b>Suggested beer:</b></h3><p>{{brewers_tips}}</p>
      </div>
  </div>